<!-- Custom Styles -->
<style>
    body { font-family: "Arial", Verdana, Tahoma; }

    h1 { font-size: 16pt; font-weight: bold; padding: 0.5em; }
    h4 { font-size: 9pt; font-weight: bold; padding: 0.5em; }

    table { width: 100%; border: 1px #000 solid; margin: 16px auto; }
    table tr { border-bottom: 1px #000 solid;  }
    table tr:last-child { border-bottom: none; }
    table tr td { padding: 10px; border-right: 1px #000 solid; width: 50%; }
    table tr td.quarterWidth { width: 25%; }
    table tr td:last-child { border-right: none; }
    table tr td p { display: block; padding: 0px 0px 5px; margin: 0; }

    table.twoBodyTableTop { border-bottom: none; margin-bottom: 0px; }
    table.twoBodyTableBottom { margin-top: 0px; }

    table.columnContent { border: none; margin: 0px 0px; }
    table.columnContent tr { border: none; }
    table.columnContent tr td { border: none; }

    ol, p { font-size: 9pt; margin: 0 0 1em; display: inline-block; }

    ol { list-style-type: upper-alpha; list-style-position:outside; display: block; width: 98%; float: right; }
    ol li { display: list-item; padding: 0 0 1em; }
    ol li div.list-content { float: right; }
    ol li ol { list-style-type: decimal; padding: 0 0 1em 1em;  }

    strong { font-weight: bold; }

    .textCenter { text-align: center; }
    .tableLabel { font-size: 8pt; font-weight: bold; }

    #documentContainer { width: 90%; max-width: 468pt; margin: 0 auto; }
    #imageHeader img { width: 150px; margin: 2em 0 0; }

    .contentRow { width: 100%; display: flex; flex-direction: row; flex-wrap: nowrap; justify-content: space-evenly; }
    .contentRow .contentColumn { display: block; float: left; width: 50%;}
    /*.contentRow .contentColumn.halfWidth { width: 49%; }*/
    .contentRow .contentColumn:first-child { padding: 0 0.5em 0 0; }
    .contentRow .contentColumn:last-child { padding: 0 0 0 0.5em; }

</style>